$SQLQuery = "SELECT TOP 25 cs-username As User, COUNT(*) as Hits  INTO '" + $Destination + "' FROM  '\\server1example\d$\IISLogs\W3SVC35982320\example.log','\\server2example\d$\logfiles\W3SVC35982320\example.log' WHERE User Is Not Null GROUP BY User "
